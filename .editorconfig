# http://editorconfig.org
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = crlf

trim_trailing_whitespace = true
insert_final_newline = true

[*.csproj]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.sh]
end_of_line = lf

[.buildrc]
end_of_line = lf
